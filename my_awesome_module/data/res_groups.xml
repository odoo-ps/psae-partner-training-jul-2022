<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="group_lucky_salesperson" model="res.groups">
            <field name="name">Lucky Salesperson</field>
            <field name="implied_ids" eval="[(4, ref('sales_team.group_sale_salesman'))]"/>
            <field name="category_id" ref="base.module_category_sales_sales"/>
        </record>
        <record id="group_read_only_users" model="res.groups">
            <field name="name">Read-Only Users</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user')), (4, ref('account.group_account_readonly'))]"/>
            <field name="category_id" ref="base.module_category_sales_sales"/>
        </record>
    </data>
</odoo>
